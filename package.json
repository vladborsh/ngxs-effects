{
  "name": "ngxs-effects",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladborsh/ngxs-effects.git"
  },
  "main": "./dist/ngxs-effect/bundles/ngxs-effect.umd.js",
  "types": "./dist/ngxs-effect/ngxs-effect.d.ts",
  "files": [
    "dist/ngxs-effect"
  ],
  "keywords": [
    "redux",
    "state",
    "ngxs",
    "angular",
    "ng",
    "effects",
    "ngxs-effects",
    "ngxs-plugin",
    "ngxs-ngrxs-effects"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo-app",
    "lint": "npm run ngxs-effect:lint",
    "test": "npm run ngxs-effect:test:watch",
    "build": "npm run ngxs-effect:build",
    "commit": "git-cz",
    "coveralls": "cat coverage/ngxs-effect/lcov.info | coveralls",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "demo-app:build": "ng build demo-app",
    "demo-app:test": "ng test demo-app",
    "demo-app:lint": "npx eslint -c .eslintrc.js 'projects/demo-app/**/*.ts' --fix",
    "ngxs-effect:build": "ng build ngxs-effect",
    "ngxs-effect:test": "ng test ngxs-effect --configuration=ci",
    "ngxs-effect:lint": "npx eslint -c .eslintrc.js 'projects/ngxs-effect/**/*.ts' --fix",
    "ngxs-effect:test:watch": "ng test ngxs-effect",
    "release": "standard-version",
    "release:patch": "npm run build && npm run release -- --release-as patch",
    "release:minor": "npm run build && npm run release -- --release-as minor",
    "release:major": "npm run build && npm run release -- --release-as major"
  },
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@ngxs/logger-plugin": "~3.6.2",
    "@ngxs/store": "~3.6.2",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~6.5.5",
    "tslib": "~2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.0.9",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "codelyzer": "~5.2.2",
    "commitizen": "^4.1.2",
    "conventional-changelog-cli": "^2.0.34",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "~1.5.4",
    "karma-mocha-reporter": "^2.2.5",
    "lint-staged": "^10.2.8",
    "ng-packagr": "^9.1.5",
    "protractor": "~7.0.0",
    "standard-version": "^8.0.0",
    "ts-node": "~8.10.2",
    "tsickle": "^0.38.1",
    "tslint": "^6.1.2",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --config ./lint-staged.config.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
